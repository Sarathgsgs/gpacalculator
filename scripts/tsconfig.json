{
    "compilerOptions": {
        "target": "ES2022",
        "module": "ESNext",
        "moduleResolution": "Bundler",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "noEmit": true,
        "skipLibCheck": true,
        "strict": true,
        "types": [
            "node"
        ]
    },
    "include": [
        "./**/*.ts"
    ]
}